        "config": {
            "metadata": {
                "fly_process_group": "cron"
            },
            "auto_destroy": true,
            "disable_machine_autostart": true,
            "guest": {
                "cpu_kind": "shared",
                "cpus": 1,
                "memory_mb": 512
            },
            "image": "docker-hub-mirror.fly.io/library/nginx:latest",
            "restart": {
                "max_retries": 1,
                "policy": "no"
            }
        }
